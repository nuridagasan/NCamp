<% layout('layouts/boilerplate') %>
    <h1>All Campsites</h1>
    <a href="/campsites/new">New Campsite</a>
    <% for(let campsite of campsites) { %>
        <div>
            <ul>
                <li>
                    <%= campsite.title %>
                </li>
                <li>
                    <%= campsite.location %>
                </li>
                <a href="/campsites/<%=campsite._id%>">More.</a>
            </ul>
        </div>
        <% } %>